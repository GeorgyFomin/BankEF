<ContentControl x:Class="BankEF.ListControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BankEF" xmlns:viewmodels="clr-namespace:BankEF.ViewModels" d:DataContext="{d:DesignInstance Type=viewmodels:ListViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="{StaticResource mainBckgr}">
        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="100" TextWrapping="Wrap" Text="List" Foreground="{StaticResource textColor}"/>
    </Grid>
</ContentControl>
